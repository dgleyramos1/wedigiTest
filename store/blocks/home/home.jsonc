      "rich-text#altadesktop",
      "slider-layout#homedesktop",
      "flex-layout.row#bannersdesktop"
    ]
  },
  "flex-layout.row#bannersdesktop": {
    "props": {
      "blockClass": "containerBanner"
    },
    "children": [
      "flex-layout.col#banner1desktop",
      "flex-layout.col#banner2desktop"
    ]
  },
  "flex-layout.col#banner1desktop":{
    "props": {
      "blockClass": "colunamini"
    },
      "children": [
        "info-card#banner1desktop"
      ]
  },
  "flex-layout.col#banner2desktop":{
    "props": {
      "blockClass": "colunamini"
    },
    "children": [
      "info-card#banner2desktop"
    ]
},
  "info-card#banner1desktop": {
    "props": {
      "id": "bannermini",
      "isFullModeStyle": true,
      "imageUrl": "https://raw.githubusercontent.com/dgleyramos1/wedigiTest/main/img/nostalgia.png",
      "headline": "Sessão Nostalgia",
      "subhead": "Encontre os jogos favoritos da sua infância.",
      "callToActionText": "Saiba mais",
      "callToActionUrl": "/sale/d",
      "blockClass": "bannerMini"
    }
  },
  "info-card#banner2desktop": {
    "props": {
      "id": "bannermini",
