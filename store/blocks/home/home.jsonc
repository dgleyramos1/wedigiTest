{
  "store.home": { 
    "blocks": [
      "responsive-layout.desktop",
      "responsive-layout.phone"
    ]
  },
  "responsive-layout.desktop": {
    "children": [
      "info-card#paineldesktop",
      "rich-text#destaquesdesktop",
      "slider-layout#homedesktop",
      "rich-text#plataformadesktop",
      "flex-layout.row#plataformas",
